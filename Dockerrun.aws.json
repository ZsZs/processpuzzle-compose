{
   "AWSEBDockerrunVersion": 2,
   
   "containerDefinitions": [
   {
      "name": "processpuzzle-proxy",
      "image": "zsuffazs/processpuzzle-proxy",
      "essential": true,
      "memory": 128,
      "portMappings": [{
          "hostPort": 80,
          "containerPort": 80
        }],
      "links": [
        "processpuzzle-core"
      ],
      "mountPoints": []
    },
   
   {
      "name": "processpuzzle-core",
      "image": "zsuffazs/processpuzzle-core:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [{
        "hostPort": 9125,
        "containerPort": 9125
      }],
      "mountPoints": []
    }]
}